	<style>
		.panel-queryBar {padding: 10px 10px 0;}
		.form-inline .form-group {margin-bottom: 10px;}
		.dataListPanel.center {height: calc(100% - 50px);padding: 10px;}
		.scrollbar-macosx {
			height: 100%;
		}
		[type='calendar'] {
			width: auto;
		}
	</style>
</head>
<body>
<div class="panel-wrapper" style="position: relative;">
	<div class="panel-layout" layout="center">
		<div class="panel-queryBar">
			<form class="form-inline">
				<div class="form-group form-group-sm">
					<div type="calendar">
						<i class="glyphicon glyphicon-calendar" type="icon"></i>
						<input type="text" class="form-control" id="khxx-datetimeStart" placeholder="开始时间">
					</div>
					<label class="addTimePeriod" db-control></label>
					<div type="calendar">
						<i class="glyphicon glyphicon-calendar" type="icon"></i>
						<input type="text" class="form-control" id="khxx-datetimeEnd" placeholder="结束时间">
					</div>
				</div>
				<div class="form-group form-group-sm">
					
					<a class="btn btn-primary btn-sm" id="select_khxx">查询</a>
					<a class="btn btn-primary btn-sm" id="Export_khxx">导出</a>
				</div>
			</form>
		</div>
		<div class="dataListPanel center">
			<div id="khxxTable"></div>
		</div>
	</div>
</div>

