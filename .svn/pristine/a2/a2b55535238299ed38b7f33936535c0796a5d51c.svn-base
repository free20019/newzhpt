.panel-title .icon-arrowTria[ip-type='right'] {-webkit-transform: rotateZ(-90deg);-ms-transform: rotate(-90deg);transform: rotateZ(-90deg);-webkit-transition: -webkit-transform .3s ease-in-out;transition: -webkit-transform .3s ease-in-out;-o-transition: transform .3s ease-in-out;transition: transform .3s ease-in-out;transition: transform .3s ease-in-out, -webkit-transform .3s ease-in-out;}
.panel-title[ip-type='attach'] {width: 30px;}
