	<style>
		.panel-queryBar {padding: 10px 10px 0;}
		.form-inline .form-group {margin-bottom: 10px;}
		.dataListPanel.center {height: calc(100% - 50px);padding: 10px;}
		[type='calendar'] {width: auto;}

		/* dialog style */
		#zpsjcx-dialog-wfnr .form-control, #zpsjcx-dialog-jieg .form-control {width: 428px;height: 50px;}
		.modal-body form {}
		.modal-body .control-label {display: inline-block;width: 6em;text-align: right;}
		.modal-body .form-control {display: inline-block;}
		textarea.form-control {resize: none;}
		.ip-type-input div.form-control, .ip-type-text input.form-control, .ip-type-text select.form-control, .ip-type-text textarea.form-control, .ip-type-text .select2.select2-container {display: none;}
		.ip-type-text div.form-control {width: 168px;}
	</style>
</head>
<body>
<div class="panel-wrapper" id="root" style="position: relative;">
	<div class="panel-layout" layout="center">
		<div class="panel-queryBar">
			<form class="form-inline">
				<div class="form-group form-group-sm">
					<div type="calendar">
						<i class="iconfont icon-chepai" type="icon"></i>
						<select class="form-control input-sm select2" id="zpsjcx-cphm" data-placeholder="车牌号" style="width:188px;padding-left: 30px">
							<option/>
						</select>
					</div>
				</div>
				<div class="form-group form-group-sm">
					<div type="calendar">
					<i class="iconfont icon-dingwei" type="icon"></i>
						<select class="form-control input-sm select2" id="zpsjcx-address" data-placeholder="场站" style="width:188px;padding-left: 30px">
						</select>
					</div>
				</div>
				<div class="form-group form-group-sm" style="line-height: 0;">
					<div type="calendar">
						<i class="iconfont icon-qiye" type="icon"></i>
						<!--<select class="form-control input-sm select2" id="zpsjcx-gs" data-placeholder="业户" style="width:188px;padding-left: 30px">
							<option/>
						</select>-->
						<tree-select class="tw-treeselect" v-model="yehu" :options="yehuOptions" multiple placeholder="业户" style="width: 188px;"></tree-select>
					</div>
				</div>
				<div class="form-group form-group-sm">
					<div type="calendar">
					<i class="glyphicon glyphicon-calendar" type="icon"></i>
						<select class="form-control input-sm select2" id="zpsjcx-option" style="width:188px;padding-left: 30px">
						</select>
					</div>
				</div>
				<div class="form-group form-group-sm">
					<div type="calendar">
						<i class="glyphicon glyphicon-calendar" type="icon"></i>
						<input type="text" class="form-control input-sm" id="zpsjcx-stateTime" placeholder="开始时间">
					</div>
				</div>
				<label>-</label>
				<div class="form-group form-group-sm">
					<div type="calendar">
						<i class="glyphicon glyphicon-calendar" type="icon"></i>
						<input type="text" class="form-control input-sm" id="zpsjcx-endTime" placeholder="结束时间">
					</div>
				</div>
				<div class="form-group form-group-sm">
						<input type="checkbox" id="check">
						<span>一分钟无重复</span>
				</div>
				<div class="form-group form-group-sm">
					<a class="btn btn-primary btn-sm" id="zpsjcx-select">查询</a>
					<a class="btn btn-primary btn-sm" id="zpsjcx-dc">导出</a>
				</div>
			</form>
		</div>
		<div class="dataListPanel center">
			<div id="zpsjcxTable"></div>
		</div>
	</div>
	<div id="zpsjbigImage" class="bigImage">
		<img src="" id="zpsjbig" class="big" width="auto" height="auto"> 
		
		<div id="zpsjimagetool" class="imagetool">
			<div class="rote" id="zpsjleft-rote">顺时针旋转</div>
			<div class="rote" id="zpsjright-rote">逆时针旋转</div>
			<div class="scale" id="zpsjbigger">放大</div>
			<div class="scale" id="zpsjsmaller">缩小</div>
			<div id="zpsjclose" class="closed">关闭</div>
		</div>
	</div>
</div>
<!--<script src="./zpsjcx.js"></script>-->
<script>
	(function(Vue, $) {
		var vm = new Vue({
			el: '#root',
			data: function () {
				return {
					yehu: [],
					yehuOptions: [
						{
							id: 'a', label: 'a',
							children: [
								{id: 'aa', label: 'aa',},
								{id: 'ab', label: 'ab',},
								{id: 'ac', label: 'ac',},
								{id: 'ad', label: 'ad',},
								{id: 'ae', label: 'ae',},
								{id: 'af', label: 'af',},
								{id: 'ag', label: 'ag',}
							],
						},
						{id: 'b', label: 'b',},
						{id: 'c', label: 'c',}
					]
				}
			},
			mounted: function () {
				var _this = this;
				this.$nextTick(function () {
				});
			},
			methods: {},
			components: {
				'tree-select': VueTreeselect.Treeselect
			}
		});
		console.info('init:',vm)
	})(Vue, jQuery);
</script>
</body>
</html>
