            public void run() {
//            		System.out.println("-----huang-----");
            		monitorlist = setsafety();
					redisService.set(Monitor.safetyMonitor,"",FastJsonUtil.toJSONString(monitorlist));
//					System.out.println("-----xianle ----");
				}
        }, 1, 30, TimeUnit.SECONDS);
	}
	
