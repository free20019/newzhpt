(function (Vue, _, $) {
		var vm = new Vue({
			el: '#root',
			data: function () {
				return {
					type: 1,
					visibleLength: 0,
					currentPage: 0,
					queryLicensePlate: '',
					vehicleList: [],
					wxclList: [],
					//高德地图海量点ui
					PointSimplifier:null,
					pointSimplifierIns:null,
					groupStyleMap:{},
					id:0,
					queryServiceType: '',
